    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
