    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "maintenance:daily": "tsx scripts/dailyMaintenance.ts",
    "maintenance:smart": "tsx scripts/smartDailyStartup.ts",
    "test:maintenance": "tsx scripts/testMaintenance.ts",
    "migrate:multitenant": "tsx scripts/migrateToMultiTenant.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/cron": "^2.0.1",
    "@types/node-fetch": "^2.6.13",
